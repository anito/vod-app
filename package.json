{
  "name": "vod-manager",
  "description": "VoD Manager",
  "version": "2.2.1",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "run-p start:dev",
    "dev:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "prepare": "smui-theme compile static/smui.css -i src/theme",
    "start": "node __sapper__/build",
    "start:dev": "sapper dev --port 3001 --host",
    "build": "npm run build:tailwind && sapper build --legacy",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build:no-tailwind": "sapper build --legacy",
    "export": "sapper export --legacy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "site-kit": "npm uninstall @anito/site-kit && npm install anito/site-kit --save-dev",
    "setup": "npm run setup:install && npm run setup:build",
    "setup:install": "pnpm install",
    "setup:build": "pnpm run build "
  },
